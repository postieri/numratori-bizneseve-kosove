{
    "name": "kosovo-business-registry",
    "version": "1.0.0",
    "description": "A web application to search and display information from the Kosovo Business Registry",
    "main": "server.js",
    "scripts": {
      "start": "node server.js",
      "dev": "nodemon server.js",
      "build": "sass src/styles.scss:public/css/styles.css",
      "watch:sass": "sass --watch src/styles.scss:public/css/styles.css",
      "postinstall": "npm run build"
    },
    "keywords": [
      "kosovo",
      "business",
      "registry",
      "aleph",
      "api"
    ],
    "author": "Luan Kerleshi - postieri XYZ L.L.C.",
    "license": "MIT",
    "dependencies": {
      "express": "^4.17.1"
    },
    "devDependencies": {
      "nodemon": "^2.0.7",
      "sass": "^1.62.1"
    },
    "engines": {
      "node": "20.x"
    }
  }
